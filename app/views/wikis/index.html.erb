<%= content_for :header_title, 'Wiki' %>

<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <div class="block">
      <div class="block-title clearfix">
        <h2>Wiki List</h2>
      </div>

        <div id="wikis-list">
          <div class="block-content-full">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-vcenter remove-margin-bottom">
                <tbody>
                  <tr>
                    <td><h3>You can create a page <%= link_to 'here', new_wiki_path %>.</h3></td>
                  </tr>
                  <%= render @wikis %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
    </div>
  </div>
</div>
