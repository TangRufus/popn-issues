<tr>
  <td><strong>@<%= user.username %></strong></td>
  <td><%= mail_to user.email, user.email %></td>
  <td>
    <% if user.confirmed? %>
      <%= user.last_sign_in_at ? local_time_ago(user.current_sign_in_at) : 'Never signed in'%>
    <% else %>
      <%= 'Not comfirmed yet'%>
    <% end %>
  </td>
</tr>
